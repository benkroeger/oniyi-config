{
  "name": "oniyi-config",
  "version": "1.1.2",
  "description": "Simple config file loader",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "Benjamin Kroeger",
    "email": "benjamin.kroeger@gmail.com",
    "url": "http://benjamin-kroeger.de"
  },
  "main": "lib/index.js",
  "keywords": [
    "oniyi",
    "config",
    "loader"
